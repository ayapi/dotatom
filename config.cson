    showInvisibles: true
    softWrap: true
    undoGroupingInterval: 1
  "exception-reporting":
    userId: "d82875f5-8c23-7271-71b1-89655f81fd84"
  linter:
    showErrorPanel: false
  "markdown-preview":
    liveUpdate: false
  welcome:
    showOnStartup: false
  whitespace:
