"*":
  "atom-ternjs": {}
  "autocomplete-plus": {}
  "bracket-matcher": {}
  core:
    automaticallyUpdate: false
    disabledPackages: [
      "spell-check"
      "tree-view"
      "wrap-guide"
    ]
    packagesWithKeymapsDisabled: [
      "symbols-view"
    ]
    projectHome: "/home/ayapi/.atom/packages-custom"
    themes: [
      "ayapi-ui"
      "monokai"
    ]
  "cursor-blink-interval":
    cursorBlinkInterval: 1200
  editor:
    invisibles:
      cr: "v"
      eol: "ï½£"
      space: "_"
      tab: ">"
